<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="800.0" minWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GameEngine">
   <children>
      <ImageView fitHeight="800.0" fitWidth="900.0" pickOnBounds="true">
         <image>
            <Image url="@River.jpg" />
         </image>
      </ImageView>
      <Button fx:id="moveBtn" layoutX="385.0" layoutY="138.0" mnemonicParsing="false" onAction="#btnAction" prefHeight="26.0" prefWidth="130.0" text="Move Boat" />
      <ImageView fx:id="boatImg" fitHeight="81.0" fitWidth="210.0" layoutX="667.0" layoutY="692.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Boat.jpg" />
         </image></ImageView>
      <ImageView fx:id="herbiImg" fitHeight="112.0" fitWidth="49.0" layoutX="243.0" layoutY="481.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="farmerImg" fitHeight="108.0" fitWidth="61.0" layoutX="195.0" layoutY="489.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="plantImg" fitHeight="122.0" fitWidth="61.0" layoutY="611.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="carniImg" fitHeight="81.0" fitWidth="77.0" layoutX="103.0" layoutY="543.0" pickOnBounds="true" preserveRatio="true" />
      <HBox alignment="CENTER" layoutX="98.0" layoutY="38.0" prefHeight="100.0" prefWidth="704.0" spacing="50.0">
         <children>
            <Button fx:id="resetBtn" mnemonicParsing="false" onAction="#resetGame" text="Reset" />
            <Button fx:id="undoBtn" mnemonicParsing="false" onAction="#undo" text="Undo" />
            <Button fx:id="redoBtn" mnemonicParsing="false" onAction="#redo" text="Redo" />
            <Button fx:id="instBtn" mnemonicParsing="false" onAction="#instructions" text="Instructions" />
            <Button fx:id="exitBtn" mnemonicParsing="false" onAction="#startBtnAction" text="Back" />
            <Button mnemonicParsing="false" onAction="#saveGame" text="Save" />
         </children>
      </HBox>
      <ImageView fx:id="farmer1Img" fitHeight="108.0" fitWidth="61.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="farmer2Img" fitHeight="108.0" fitWidth="61.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="farmer3Img" fitHeight="108.0" fitWidth="61.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="farmer4Img" fitHeight="108.0" fitWidth="61.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="scoreLabel" alignment="CENTER" layoutX="368.0" layoutY="228.0" prefHeight="46.0" prefWidth="163.0" text="Label">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <ImageView fx:id="animalImg" fitHeight="108.0" fitWidth="61.0" layoutX="10.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
